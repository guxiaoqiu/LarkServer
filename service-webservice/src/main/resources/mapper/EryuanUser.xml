	</insert>

    <!--查询二院用户 -->
    <select id="queryEryuanUserByNoCount" parameterType="String" resultType="int">
        SELECT count(*)
        FROM ERYUANUSER eu
        WHERE eu.P_ID = #{pId}
        </select>

