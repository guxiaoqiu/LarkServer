    <!-- 新增二院用户 -->
    <insert id="saveEryuanUser">
		INSERT INTO ERYUANUSER (ID,H_ID, SYS_ID, CASIC_ORG_CODE, OPERATOR_NAME, EMP_CODE, P_ID,SECRET_LEVEL,BIRTH_DATE,GENDER,O_TEL,O_EMAIL,WORK_POST,TEC_POST,ORDER_ID,SYNC_TYPE,DISP_TIME,STATUS,REMARK,REFA,REFB,ORG_NAME,IS_SUCCESS) VALUES
